<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
    <system.web>
        <authorization>
            <allow roles="Admin" />
            <deny users="*" />
        </authorization>
    </system.web>
  <location path="Default.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin" />
        <allow roles="Devs" />
        <deny users="*" />
      </authorization>
    </system.web>
  </location>
  <location path="admin/Errors.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin" />
        <allow roles="Devs" />
        <deny users="*" />
      </authorization>
    </system.web>
  </location>
</configuration>
